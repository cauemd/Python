variable = input('Type something: ')
print('The primitive data type of that is :', type(variable))
print('Is it only composed of spaces? ', variable.isspace())
print('Is it a number? ', variable.isnumeric())
print('Is it a word or a letter? ', variable.isalpha())
print('Is it composed of number &&/|| letters? ', variable.isalnum())
print('Is it written in all upper case? ', variable.isupper())
print('Is it written in all lower case? ', variable.islower())
print('Is it capitalized? ', variable.istitle())
